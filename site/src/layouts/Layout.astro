---
export interface Props {
  title: string;
}
const { title } = Astro.props;
import '../styles/global.css';
---
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <title>{title}</title>
  </head>
  <body class="prose mx-auto p-4">
    <slot />
  </body>
</html>
